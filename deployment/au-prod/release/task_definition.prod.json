{
  "containerDefinitions": [
    {
      "name": "main_container",
      "image": "912195358089.dkr.ecr.ap-southeast-2.amazonaws.com/webui-pms",
      "portMappings": [
        { "containerPort": 4200, "hostPort": 4200, "protocol": "tcp" }
      ],
      "essential": true
    }
  ],
  "family": "webui-pms",
  "cpu": "2048",
  "memory": "4096",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["EC2", "FARGATE"],
  "executionRoleArn": "arn:aws:iam::912195358089:role/ecsTaskExecutionRole"
}
